<%@page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<link rel="stylesheet" type="text/css" href="<%=request.getContextPath() %>/css/upload/upload.css?appV=${appV}">

<div class="pic-child-container upload-container">
	<div class="pic-child-content upload-content">
		<div class="content-head upload-head">
			<div class="content-title search-description">
				图片上传
			</div>
		</div>
		<div class="content-body upload-body">
			<div class="file-upload-bar">
				<div class="account-select"><input id="account_bind_inp" type="text" class="form-control from-small" readonly placeholder="—指定账号—"></div>
				<div id="filePicker" class="file-toolbar">选择图片</div>
				<div id="upload_files_btn" class="webuploader-pick file-toolbar custom-toolbar">开始上传</div>
				<div id="del_files_btn" class="webuploader-pick file-toolbar custom-toolbar">一键清空</div>
				<div id="file_status" class="status-toolbar"></div>
				<div id="process_status" class="process-toobar"></div>
			</div>
			<div class="file-upload-area">
				<div id="file_upload_panel" class="file-upload-content">
					 <!--用来存放item-->
				    <div id="fileList" class="uploader-list"></div>
				    <div id="upload_description" class="upload-description">
				    	<div id="filePicker2" class="file-picker-inner"></div>
				    </div>
				</div>
			</div>
		</div>
	</div>
</div>

<!-- 引入自身JS-->
<script type="text/javascript" src="${contextPath}/js/upload/upload.js?appV=${appV}"></script>



























