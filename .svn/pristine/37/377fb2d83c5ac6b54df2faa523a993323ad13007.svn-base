<%@page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<link rel="stylesheet" type="text/css" href="${contextPath }/css/search/search.css?appV=${appV}">

<!-- 隐藏域 -->
<input type="hidden" id="del_img_permission" name="del_img_permission" value="${del_img_permission }"/>

<div class="pic-child-container search-container">
	<div class="pic-child-content search-content">
		<div class="content-head search-head">
			<div class="content-title search-description">
				图片查询
			</div>
			<div id="img_search_area" class="search-box">
				<i class="icon-search pic-icon-search"></i>
				<input id="keyWord" name="keyWord" type="text" class="search-condition" placeholder="输入关键字搜索"/>
				<div class="search-btn">搜索</div>
			</div>
		</div>
		<div class="content-body search-body">
			<div id="img_search_grid"></div>
		</div>
	</div>
</div>
<!-- 分页插件 JS-->
<script type="text/javascript" src="${contextPath }/plugins/laypage/laypage.js?appV=${appV}"></script>
<!-- 引入自身JS-->
<script type="text/javascript" src="${contextPath }/js/search/search.js?appV=${appV}"></script>





























