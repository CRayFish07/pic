package com.hengzhiyi.it.pic.vo;

import java.util.Date;

/**
 * 图片实体类
 * 
 * @author liutianlong
 *
 */
public class ImageVO extends BaseVO
{
	private static final long serialVersionUID = 1L;

	// 圖片名称
	private String name;
	// 存储的绝对路径
	private String absolutePath;
	// 存储的相对路径
	private String relativePath;
	// 生成的HTTP路径
	private String url;
	// 缩放版的URL
	private String smallUrl;
	// 扩展名
	private String extension;
	// 上传时间
	private Date uploadDate;
	// 上传人ID
	private String userId;
	// 上传人名称
	private String userName;
	// 备注
	private String remark;
	// 缩放文件的绝对路径
	private String smallAbsolutePath;
	// 缩放文件的相对路径
	private String smallRelativePath;
	// 访问图片的上下文
	private String contextPath;
	// 账号ID
	private String accountId;
	
	public String getName()
	{
		return name;
	}
	public void setName(String name)
	{
		this.name = name;
	}
	public String getAbsolutePath()
	{
		return absolutePath;
	}
	public void setAbsolutePath(String absolutePath)
	{
		this.absolutePath = absolutePath;
	}
	public String getRelativePath()
	{
		return relativePath;
	}
	public void setRelativePath(String relativePath)
	{
		this.relativePath = relativePath;
	}
	public String getUrl()
	{
		return url;
	}
	public void setUrl(String url)
	{
		this.url = url;
	}
	public String getSmallUrl()
	{
		return smallUrl;
	}
	public void setSmallUrl(String smallUrl)
	{
		this.smallUrl = smallUrl;
	}
	public String getExtension()
	{
		return extension;
	}
	public void setExtension(String extension)
	{
		this.extension = extension;
	}
	public Date getUploadDate()
	{
		return uploadDate;
	}
	public void setUploadDate(Date uploadDate)
	{
		this.uploadDate = uploadDate;
	}
	public String getUserId()
	{
		return userId;
	}
	public void setUserId(String userId)
	{
		this.userId = userId;
	}
	public String getUserName()
	{
		return userName;
	}
	public void setUserName(String userName)
	{
		this.userName = userName;
	}
	public String getRemark()
	{
		return remark;
	}
	public void setRemark(String remark)
	{
		this.remark = remark;
	}
	public String getSmallAbsolutePath()
	{
		return smallAbsolutePath;
	}
	public void setSmallAbsolutePath(String smallAbsolutePath)
	{
		this.smallAbsolutePath = smallAbsolutePath;
	}
	public String getSmallRelativePath()
	{
		return smallRelativePath;
	}
	public void setSmallRelativePath(String smallRelativePath)
	{
		this.smallRelativePath = smallRelativePath;
	}
	public String getContextPath()
	{
		return contextPath;
	}
	public void setContextPath(String contextPath)
	{
		this.contextPath = contextPath;
	}
	public String getAccountId()
	{
		return accountId;
	}
	public void setAccountId(String accountId)
	{
		this.accountId = accountId;
	}
}
