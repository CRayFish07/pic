
.upload-content .upload-head{
	width: 100%;
	height: 40px;
	background: #add8e6;
	/* border:solid 1px #add8e6; */
	border-radius:4px 4px 0px 0px;
}
.upload-content .upload-body{
	overflow:hidden;
	border:solid 1px #add8e6;
	border-radius:0px 0px 4px 4px;
}

.file-upload-area{
	overflow-x: hidden;
	overflow-y: auto;
	width:100%;
	height: calc(100% - 30px);
    margin: 0px;
    padding: 10px
}
.file-upload-bar{
	width: 100%;
	height: 30px;
	position:relative;
	border-bottom-color: #add8e6;
	border-bottom-style: solid;
	border-bottom-width: 1px;
}
.file-upload-area .file-upload-content{
	width: auto;
	height: auto;
	min-height: 100%;
/* 	margin:10px; */
	padding:5px; 
	position:relative;
	border:3px dashed #E6E6E6;
}
.file-upload-area .webuploader-dnd-over{
	border: 3px dashed #999999;
}
.file-upload-bar .file-toolbar{
	margin:3px auto 3px 10px;
	float:left;
	width:auto;
}
.file-upload-bar .status-toolbar{
	height: 100%;
	line-height:30px;
	float:right;
	text-align:center;
	margin:auto 10px;
	display:inline-block;
	font-size: 14px;
	color: #666;
	/* border:solid 1px red; */
}
.file-upload-bar .process-toobar{
	float: right;
	height:100%;
	position:relative;
	color:#00A2D4;
	margin:auto 10px;
	font-size:11px;
	display: none;
}
.file-upload-bar .process-toobar .process-panel{
	height:10px;
	width: 100%;
	padding:0px;
	margin:0px;
	text-align:center;
	color:#ff0000;
	line-height:8px;
	position:relative;
	border:solid 1px #00b7ee;
	border-radius:2px; 
	font-size: xx-small;
}
.file-upload-bar .process-toobar .process-panel div{
	display: inline-block;
	height:100%;
	width: 10%;
	padding:0px;
	margin:0px;
	position:absolute;
	left:0px;
	top:0px;
	z-index:-1;
	background: #00b7ee;
}
.file-upload-bar .custom-toolbar{
	display: none;
}
.file-upload-bar .custom-toolbar:HOVER{
	 background: #00A2D4;
}
.file-upload-bar .disable{
	background: #C0C0C0;
	cursor: default;
}
.file-upload-bar .webuploader-pick{
    padding: 2px 10px;
    height: 23px;
}
.file-upload-content .upload-description{
	width: 300px;
	height: 200px;
	/* margin: 100px auto; */
	margin: auto;
	position: relative;
	background: url(../../images/image.png) no-repeat top center;
	border:solid 1px transparent;
	color:#CCC;
	font-size: 14px;
	text-align: center;
	display: block;
}
.file-upload-content .upload-description .file-picker-inner{
	position:relative;
	margin-top: 100px;
	width: 100%;
	font-size: 18px;
}
.uploader-list .upload-item{
	position: relative;
}
.uploader-list .upload-item:HOVER{
	border:solid 1px #000;
}
.uploader-list .upload-item .error{
	left: 4px;
	right: 21px;
	font-size: 8px !important;
	padding: 0px;
	margin: 0px;
}
.uploader-list .upload-item:HOVER>i{
	display: block;
}
.uploader-list .upload-item .del-item-btn{
	position: absolute;
	top:0px;
	right:0px;
	color:#000;
	margin:0px;
	padding:0px;
	cursor: pointer;
	display: none;
}
.uploader-list .upload-item .del-item-btn:HOVER{
	color: red;
}
.uploader-list .upload-item .del-item-btn::BEFORE{
	z-index: 999;
}
.account-select{
    width: auto;
    position: relative;
    display: inline-block;
    margin-top: 3px;
    margin-left: 10px;
    float: left;	
}
.account-select input{
    width: 150px;
    padding: .5px 30px .5px 5px;
    height: auto;
    border-radius: 1px 1px 0px 0px;
    border: solid 1px #00A2D4;
}
.account-select input:FOCUS{
	border-color: #00A2D4;
	-webkit-box-shadow: none;
	box-shadow: none;
}
.account-select input[readonly]{
	background-color: transparent;
}
.ztree-arrow{
	height: 20px;
}
.ztree-clear{
	top: 2px;
}
.ztree li a.curSelectedNode{
	background-color: #00A2D4;
    border-color: #00A2D4;
}
.ztree-open{
	border: solid 1px #00A2D4;
	border-bottom: none !important;
}























